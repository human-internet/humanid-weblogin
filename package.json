{
  "name": "humanid-weblogin",
  "version": "1.0.0",
  "description": "Web Log-in User Interface for humanID",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss scss/style.scss src/public/assets/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "prod": "node-sass -include-path scss/style.scss src/public/assets/css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/human-internet/humanid-weblogin"
  },
  "author": "Suhainda Librado",
  "license": "ISC",
  "devDependencies": {
    "intl-tel-input": "^17.0.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4"
  }
}
